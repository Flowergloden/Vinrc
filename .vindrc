" remote repo
 source Flowergloden/Vinrc

" default mapping
version tiny

" init mode
initmode r

" block-style caret
blockstylecaret true
blockstylecaret_mode flex

" auto window focus
autotrack_popup true

" easy-click cache
uiacachebuild true

" shell
shell wt

" mode switch
map <Esc-Esc> <to_resident>
map <Esc-[> <to_gui_normal>
map <Esc-]> <to_edi_normal>
rmap <Esc-[> <to_gui_normal>
rmap <Esc-]> <to_edi_normal>
